!Xdefaults

*dpi:				96
*rgba:				rgb
*hinting:			true
*antialias:			true
*theme:				aero-drop
*hintstyle:			hintslight
Xcursor.theme:			Human


URxvt.title: Urxvt
URxvt*font: xft:kates:pixelsize=11
!URxvt*boldFont:  -*-terminus-*-*-*-*-*-*-*-*-*-*-*-*
URxvt.cursorColor: #5E468C
URxvt.internalBorder: 4
URxvt.underlineURLs: true
URxvt.depth:      32
URxvt.foreground: white
URxvt.background: rgba:1000/1000/1000/dddd
URxvt.jumpScroll:               true 
URxvt.multiScroll:              true
URxvt.geometry:			115x57+0--1
URxvt*scrollBar: 		false
!URxvt.borderLess:		true
URxvt.fading:			0

/* Terminal colors
 * 0-7 low intensity
 * 8-15 high intensity
 */
! Black ------------------------------------------- !
    *color0:              #1D1D1D
    *color8:              #505050
! Red --------------------------------------------- !
    *color1:              #f03669
    *color9:              #c75b79
! Green ------------------------------------------- !
    *color2:              #b8e346
    *color10:              #c8e37e
! Yellow ------------------------------------------ !
    *color3:              #ffa402
    *color11:              #ffbe4a
! Blue -------------------------------------------- !
    *color4:              #02a2ff
    *color12:              #71cbff
! Magenta ----------------------------------------- !
    *color5:              #875c8d
    *color13:              #e41f66
! Cyan -------------------------------------------- !
    *color6:              #6fa9d3
    *color14:              #0078CE
! White ------------------------------------------- !
    *color7:              #d2d2d2
    *color15:              #fafafa
